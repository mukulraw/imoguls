<?xml version="1.0" encoding="utf-8"?><android.support.v4.widget.DrawerLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:id="@+id/drawer_asiana"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:openDrawer="start"    android:fitsSystemWindows="true"    tools:context="internetmoguls.com.imoguls.Asiana">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:orientation="vertical"            >            <include                layout="@layout/toolbar"/>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:orientation="vertical"                android:id="@+id/container_layout"                />        </LinearLayout>    </RelativeLayout>    <include        layout="@layout/menu_layout"        android:id="@+id/navId"        android:layout_gravity="start"        android:fitsSystemWindows="true"        android:layout_height="match_parent"        android:layout_width="268dp"/></android.support.v4.widget.DrawerLayout>